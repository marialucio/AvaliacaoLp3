@model List<AvaliacaoLp3.ViewModels.EstabelecimentoViewModel>

@{
    ViewData["Title"] = "Lojas e Quiosques";
}

<h1>Lojas e quiosques</h1>

@if(Model.Count == 0)
{
    <p>Não há nenhuma loja ou quiosque</p>
}
else
{
    <ul>
        @foreach(var estabelecimento in Model)
        {
            <li><h3>@estabelecimento.Nome</h3></li>
        }
    </ul>
}